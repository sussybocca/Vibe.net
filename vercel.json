{
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/server/:slug", "destination": "/api/serve-server?slug=:slug" },
    { "source": "/editor", "destination": "/editor.html" },
    { "source": "/explore", "destination": "/explore.html" },
    { "source": "/", "destination": "/index.html" }
  ]
}
